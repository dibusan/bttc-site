<div class="container">
  <div class="row calendar-container">
    <table class=" table table-striped table-bordered">
      <thead>
        <tr>
          <th>Date</th>
          <th>Time</th>
          <th>Spots Reserved</th>
        </tr>
      </thead>

      <tbody>
        <% @timeblocks.each do |t_block| %>
          <tr>
            <td><%= t_block.block_start_time.strftime('%A, %B %d') %></td>
            <td><%= t_block.block_start_time.strftime('%l:%M %P') %></td>
            <td><%= compress_users(t_block.users)[current_user] %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>